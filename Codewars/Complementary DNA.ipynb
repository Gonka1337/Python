    "def DNA_strand(dna):\n",
    "    dn=str()\n",
    "    for i in dna:\n",
    "        if i== 'A':\n",
    "            dn+='T'\n",
    "        elif i=='C':\n",
    "            dn+='G'\n",
    "        elif i=='T':\n",
    "            dn+='A'\n",
    "        else:\n",
    "            dn+='C'\n",
    "    return(dn)"
   ]
